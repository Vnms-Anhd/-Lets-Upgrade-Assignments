#include <iostream>
#include <cmath>
using namespace std;

bool is_prime(int n) {
    if (n <= 1) {
        return false;
    }
    for (int i = 2; i <= sqrt(n); i++) {
        if (n % i == 0) {
            return false;
        }
    }
    return true;
}

int main() {
    int A;
    cin >> A;
    if (is_prime(A)) {
        cout << A << " is a prime number." << endl;
    } else {
        cout << A << " is not a prime number." << endl;
    }
    return 0;
}
